<%
qtools.validateProperties({
	targetObject:this,
	propList:[
		{name:'viewHelper'},
		{name:'displayParameters'},
		{name:'formData'}
	],
 source:'boilerplate.view.name' });

qtools.validateProperties({
	targetObject:displayParameters,
	propList:[
		{name:'contentContainer'},
		{name:'activationButton'},
		{name:'placeholder', importance:'optional'}
	],
 source:'simple_store.main.init.ejs' });
 
qtools.validateProperties({
	targetObject:this.formData,
	propList:[
		{name:'message'},
		{name:'dataGroupName'}
	],
 source:'session.login.mainPanel' });
 
 if (window.location.host.match(/local/) || window.location.host.match(/demo/) || window.location.hash=='#tq'){
		var cardName='TQ White II',
			street='5004 Three Points Blvd',
			city='Mound',
			state='MN',
			zip='55364',
			phoneNumber='708-763-0100',
			cardNumber='8881 0000 0000 1243',
			expMonth='06',
			expYear='15',
			emailAdr='tq@justkidding.com',
			confirmEmail='tq@justkidding.com',
			name='TQ White II',
			memo='fourth grade',
			total=111;
	}
	else{
		var cardName='',
			street='',
			city='',
			state='',
			zip='',
			phoneNumber='',
			cardNumber='',
			expMonth='',
			expYear='',
			emailAdr='',
			confirmEmail='',
			name='',
			poNumber='',
			poName='',
			poPhone='',
			poEmail='';

	}
	
 %>
<table>
	<tr><td class='paymentSectionTitle' colspan='2'>Credit Card Info</td></tr>
	<tr><td  style='width:35%;'>Card Number</td><td><input class='longInput' type='text' name='<%=this.formData.dataGroupName%>[number]' value='<%=cardNumber%>' prompt='visa/mc/amex/discover/diners/jcb'></td>
	<tr><td>Expiration Date</td><td><input type='text' style='width:20%;' name='<%=this.formData.dataGroupName%>[expMonth]' value='<%=expMonth%>'  prompt='MM'><input style='margin-left:5px;width:20%;' value='<%=expYear%>' name='<%=this.formData.dataGroupName%>[expYear]' value='' prompt='YY'></td>

<tr><td style='height:5px;font-size:4px;' colspan='2'>&nbsp;</td></tr>
	<tr><td style='width:35%;'>Name on Card</td><td style='width:60%;'><input class='longInput' type='text' name='<%=this.formData.dataGroupName%>[name]' value='<%=cardName%>'></td>

<tr><td style='height:5px;font-size:4px;' colspan='2'>&nbsp;</td></tr>
	<tr><td>Organization Name</td><td><input class='longInput' type='text' name='<%=this.formData.dataGroupName%>[orgName]' value='<%=poName%>' prompt='optional'></td>

	<tr><td>Billing Street</td><td><input class='longInput' type='text' name='<%=this.formData.dataGroupName%>[street]' value='<%=street%>'></td>
	<tr><td>City, State</td><td><input type='text' style='width:60%;' name='<%=this.formData.dataGroupName%>[city]' value='<%=city%>'>, <input type='text' style='width:20%;' name='<%=this.formData.dataGroupName%>[state]' value='<%=state%>'></td>

	<tr><td>Zip</td><td><input type='text' style='width:30%;' name='<%=this.formData.dataGroupName%>[zip]' value='<%=zip%>' prompt='00000'></td>

<tr><td style='height:5px;font-size:4px;' colspan='2'>&nbsp;</td></tr>
	<tr><td style='padding-top:5px;;'>Memo</td><td><input class='longInput' type='text' name='<%=this.formData.dataGroupName%>[memo]' value='<%=memo%>' prompt='optional'></td>

</table>
